        <link rel="apple-touch-icon-precomposed" href="/apple-touch-icon-precomposed.png">
    {%block static_css%}
        <link rel="stylesheet" type="text/css" href="/css/font-awesome.min.css">
        <link rel="stylesheet" type="text/css" href="/css/ionicons.min.css">
